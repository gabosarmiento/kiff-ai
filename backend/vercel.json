{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "pipInstallArgs": "-r requirements-vercel.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 30
    }
  },
  "excludeFiles": [
    "generated_apps/**",
    "*.log",
    "*.sqlite",
    "*.db",
    "venv/**",
    "__pycache__/**",
    "*.pyc",
    ".pytest_cache/**",
    "tmp/**"
  ]
}